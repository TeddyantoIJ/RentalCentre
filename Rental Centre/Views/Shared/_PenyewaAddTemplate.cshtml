                                </li>
                                <li>
                                    <i class="fab fa-opencart"></i> Off 50%! Shop Now
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <div class="our-link">
                        <ul>
                            @{ string hidden = "hidden";
                                if (Session["username"] != null)
                                {
                                    hidden = "";
                                }
                                else
                                {
                                    hidden = "hidden";
                                }
                            }
                            <div @hidden>
                                <li>
                                    <div class="dropdown show">
                                        <a class="dropdown-item" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            Profile
                                        </a>

                                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                            <a class="dropdown-item-text" onmousemove="coloring(this)" onmouseout="colored(this)" href="@Url.Action("page_myAccount", "Penyewa")" style="color:black"><small>Lihat profile</small></a>
                                            <a class="dropdown-item-text" onmousemove="coloring(this)" onmouseout="colored(this)" href="@Url.Action("edit_myAccount", "Penyewa")" style="color:black"><small>Edit Profile</small></a>
                                            <a class="dropdown-item-text" onmousemove="coloring(this)" onmouseout="colored(this)" href="@Url.Action("edit_password", "Penyewa")" style="color:black"><small>Ubah Password</small></a>
                                            <a class="dropdown-item-text" onmousemove="coloring(this)" onmouseout="colored(this)" href="@Url.Action("hapus_myAccount", "Penyewa")" style="color:black"><small>Nonaktifkan akun</small></a>
                                        </div>
                                    </div>
                                    <!-- SCRIPT -->
                                    <script>
                                        function coloring(a) {
                                            a.style.color = "lightblue";
                                        }
                                        function colored(a) {
                                            a.style.color = "black";
                                        }
                                    </script>
                                </li>

                                <li>
                                    <a class="dropdown-item" href="#" data-toggle="modal" data-target="#logoutModal" style="color:red">
                                        Logout
                                    </a>
                                </li>
                            </div>
                            @if (Session["username"] == null)
                            {
                                <li><a data-toggle="modal" data-target="#detail" href="#">Sign-in</a></li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
