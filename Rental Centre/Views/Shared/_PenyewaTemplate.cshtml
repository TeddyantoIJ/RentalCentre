                    <ul class="cart-list">
                        @{ int total = 0;
                            if (ViewBag.cart != null)
                            {

                                foreach (var item in ViewBag.cart)
                                {
                                    foreach (var barang in ViewBag.allBarang)
                                    {
                                        if (barang.id_barang == item.id_barang)
                                        {
                                            total = total + barang.harga_sewa;
                                            <li>
                                                <a href="#" class="photo"><img src="~/Content/RoleRental/Image/Barang/@barang.link_gambar" class="cart-thumb" alt="" /></a>
